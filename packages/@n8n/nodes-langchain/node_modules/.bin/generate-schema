#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/generate-schema@2.6.0/node_modules/generate-schema/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/generate-schema@2.6.0/node_modules/generate-schema/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/generate-schema@2.6.0/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/generate-schema@2.6.0/node_modules/generate-schema/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/generate-schema@2.6.0/node_modules/generate-schema/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/generate-schema@2.6.0/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../generate-schema/bin/generate-schema" "$@"
else
  exec node  "$basedir/../generate-schema/bin/generate-schema" "$@"
fi
