#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/@n8n+typeorm@0.3.20-12_@sentry+node@9.42.1_ioredis@5.3.2_mongodb@6.11.0_@aws-sdk+creden_37ac6c08b03c56203e0dcc946ec8dafb/node_modules/@n8n/typeorm/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@n8n+typeorm@0.3.20-12_@sentry+node@9.42.1_ioredis@5.3.2_mongodb@6.11.0_@aws-sdk+creden_37ac6c08b03c56203e0dcc946ec8dafb/node_modules/@n8n/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@n8n+typeorm@0.3.20-12_@sentry+node@9.42.1_ioredis@5.3.2_mongodb@6.11.0_@aws-sdk+creden_37ac6c08b03c56203e0dcc946ec8dafb/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/@n8n+typeorm@0.3.20-12_@sentry+node@9.42.1_ioredis@5.3.2_mongodb@6.11.0_@aws-sdk+creden_37ac6c08b03c56203e0dcc946ec8dafb/node_modules/@n8n/typeorm/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@n8n+typeorm@0.3.20-12_@sentry+node@9.42.1_ioredis@5.3.2_mongodb@6.11.0_@aws-sdk+creden_37ac6c08b03c56203e0dcc946ec8dafb/node_modules/@n8n/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@n8n+typeorm@0.3.20-12_@sentry+node@9.42.1_ioredis@5.3.2_mongodb@6.11.0_@aws-sdk+creden_37ac6c08b03c56203e0dcc946ec8dafb/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@n8n/typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../@n8n/typeorm/cli-ts-node-commonjs.js" "$@"
fi
