#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.1_@types+node@20.19.11__jiti@1.21.7_postcss@8.5.6_tsx@4.19.3_typescript@5.9.2/node_modules/tsup/dist/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.1_@types+node@20.19.11__jiti@1.21.7_postcss@8.5.6_tsx@4.19.3_typescript@5.9.2/node_modules/tsup/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.1_@types+node@20.19.11__jiti@1.21.7_postcss@8.5.6_tsx@4.19.3_typescript@5.9.2/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.1_@types+node@20.19.11__jiti@1.21.7_postcss@8.5.6_tsx@4.19.3_typescript@5.9.2/node_modules/tsup/dist/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.1_@types+node@20.19.11__jiti@1.21.7_postcss@8.5.6_tsx@4.19.3_typescript@5.9.2/node_modules/tsup/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/tsup@8.5.0_@microsoft+api-extractor@7.52.1_@types+node@20.19.11__jiti@1.21.7_postcss@8.5.6_tsx@4.19.3_typescript@5.9.2/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
