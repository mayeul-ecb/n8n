#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules/@currents/playwright/dist/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules/@currents/playwright/dist/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules/@currents/playwright/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules/@currents/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules/@currents/playwright/dist/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules/@currents/playwright/dist/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules/@currents/playwright/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules/@currents/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@currents+playwright@1.15.3_bufferutil@4.0.9_magicast@0.3.5_utf-8-validate@5.0.10/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@currents/playwright/dist/bin/pwc.js" "$@"
else
  exec node  "$basedir/../@currents/playwright/dist/bin/pwc.js" "$@"
fi
