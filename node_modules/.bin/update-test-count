#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/find-test-names@1.29.7_@babel+core@7.26.10/node_modules/find-test-names/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/find-test-names@1.29.7_@babel+core@7.26.10/node_modules/find-test-names/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/find-test-names@1.29.7_@babel+core@7.26.10/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/find-test-names@1.29.7_@babel+core@7.26.10/node_modules/find-test-names/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/find-test-names@1.29.7_@babel+core@7.26.10/node_modules/find-test-names/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/find-test-names@1.29.7_@babel+core@7.26.10/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../find-test-names/bin/update-test-count.js" "$@"
else
  exec node  "$basedir/../find-test-names/bin/update-test-count.js" "$@"
fi
