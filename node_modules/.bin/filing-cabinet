#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/filing-cabinet@5.0.3/node_modules/filing-cabinet/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/filing-cabinet@5.0.3/node_modules/filing-cabinet/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/filing-cabinet@5.0.3/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/filing-cabinet@5.0.3/node_modules/filing-cabinet/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/filing-cabinet@5.0.3/node_modules/filing-cabinet/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/filing-cabinet@5.0.3/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../filing-cabinet/bin/cli.js" "$@"
else
  exec node  "$basedir/../filing-cabinet/bin/cli.js" "$@"
fi
