#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/transliteration@2.3.5/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/transliteration@2.3.5/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../transliteration/dist/bin/transliterate" "$@"
else
  exec node  "$basedir/../transliteration/dist/bin/transliterate" "$@"
fi
