#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/@redocly+cli@1.28.5_bufferutil@4.0.9_encoding@0.1.13_utf-8-validate@5.0.10/node_modules/@redocly/cli/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@redocly+cli@1.28.5_bufferutil@4.0.9_encoding@0.1.13_utf-8-validate@5.0.10/node_modules/@redocly/cli/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@redocly+cli@1.28.5_bufferutil@4.0.9_encoding@0.1.13_utf-8-validate@5.0.10/node_modules/@redocly/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@redocly+cli@1.28.5_bufferutil@4.0.9_encoding@0.1.13_utf-8-validate@5.0.10/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/@redocly+cli@1.28.5_bufferutil@4.0.9_encoding@0.1.13_utf-8-validate@5.0.10/node_modules/@redocly/cli/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@redocly+cli@1.28.5_bufferutil@4.0.9_encoding@0.1.13_utf-8-validate@5.0.10/node_modules/@redocly/cli/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@redocly+cli@1.28.5_bufferutil@4.0.9_encoding@0.1.13_utf-8-validate@5.0.10/node_modules/@redocly/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/@redocly+cli@1.28.5_bufferutil@4.0.9_encoding@0.1.13_utf-8-validate@5.0.10/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@redocly/cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../@redocly/cli/bin/cli.js" "$@"
fi
