#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.26.10_@jest+types@29.6.1_babel-jest@29.6.2_@babel+core@7.2_f020431ec16d8c2faad641887482c0dd/node_modules/ts-jest/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.26.10_@jest+types@29.6.1_babel-jest@29.6.2_@babel+core@7.2_f020431ec16d8c2faad641887482c0dd/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.26.10_@jest+types@29.6.1_babel-jest@29.6.2_@babel+core@7.2_f020431ec16d8c2faad641887482c0dd/node_modules/ts-jest/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.26.10_@jest+types@29.6.1_babel-jest@29.6.2_@babel+core@7.2_f020431ec16d8c2faad641887482c0dd/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
