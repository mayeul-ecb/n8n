#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/curlconverter@4.12.0/node_modules/curlconverter/dist/src/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/curlconverter@4.12.0/node_modules/curlconverter/dist/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/curlconverter@4.12.0/node_modules/curlconverter/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/curlconverter@4.12.0/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/curlconverter@4.12.0/node_modules/curlconverter/dist/src/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/curlconverter@4.12.0/node_modules/curlconverter/dist/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/curlconverter@4.12.0/node_modules/curlconverter/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/curlconverter@4.12.0/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../curlconverter/dist/src/cli.js" "$@"
else
  exec node  "$basedir/../curlconverter/dist/src/cli.js" "$@"
fi
