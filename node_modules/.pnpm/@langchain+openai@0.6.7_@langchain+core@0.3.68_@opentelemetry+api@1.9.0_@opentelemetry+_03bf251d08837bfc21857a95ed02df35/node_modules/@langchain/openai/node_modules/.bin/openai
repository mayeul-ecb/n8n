#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/openai@5.12.2_ws@8.18.3_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.25.67/node_modules/openai/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/openai@5.12.2_ws@8.18.3_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.25.67/node_modules/openai/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/openai@5.12.2_ws@8.18.3_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.25.67/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/openai@5.12.2_ws@8.18.3_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.25.67/node_modules/openai/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/openai@5.12.2_ws@8.18.3_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.25.67/node_modules/openai/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/openai@5.12.2_ws@8.18.3_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.25.67/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../openai@5.12.2_ws@8.18.3_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.25.67/node_modules/openai/bin/cli" "$@"
else
  exec node  "$basedir/../../../../../../openai@5.12.2_ws@8.18.3_bufferutil@4.0.9_utf-8-validate@5.0.10__zod@3.25.67/node_modules/openai/bin/cli" "$@"
fi
