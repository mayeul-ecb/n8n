#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/esprima-next@5.8.4/node_modules/esprima-next/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/esprima-next@5.8.4/node_modules/esprima-next/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/esprima-next@5.8.4/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/esprima-next@5.8.4/node_modules/esprima-next/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/esprima-next@5.8.4/node_modules/esprima-next/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/esprima-next@5.8.4/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../esprima-next@5.8.4/node_modules/esprima-next/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../../../../../esprima-next@5.8.4/node_modules/esprima-next/bin/esvalidate.js" "$@"
fi
