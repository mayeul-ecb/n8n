#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/vite@6.3.5_@types+node@20.19.11_jiti@1.21.7_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules/vite/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/vite@6.3.5_@types+node@20.19.11_jiti@1.21.7_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules/vite/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/vite@6.3.5_@types+node@20.19.11_jiti@1.21.7_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mayeul/projects/n8n/node_modules/.pnpm/vite@6.3.5_@types+node@20.19.11_jiti@1.21.7_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules/vite/bin/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/vite@6.3.5_@types+node@20.19.11_jiti@1.21.7_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules/vite/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/vite@6.3.5_@types+node@20.19.11_jiti@1.21.7_sass@1.89.2_terser@5.16.1_tsx@4.19.3/node_modules:/home/mayeul/projects/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../vite/bin/vite.js" "$@"
fi
